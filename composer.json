{
    "name": "andrewdyer/date-expression",
    "description": "A utility that can be used to create raw query date expressions for MySQL or SQLite Laravel databases",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Dyer",
            "email": "andrewdyer@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anddye\\DateExpression\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anddye\\DateExpression\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.76"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
